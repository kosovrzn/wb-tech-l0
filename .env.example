# PostgreSQL connection string
PG_DSN=postgres://user:pass@localhost:5432/orders?sslmode=disable

# Kafka consumer settings
KAFKA_BROKERS=localhost:9092
KAFKA_TOPIC=orders
KAFKA_GROUP=ordersvc

# HTTP server bind address
HTTP_ADDR=:8081

# Service tuning
WARMUP_LIMIT=1000
CACHE_CAPACITY=1000

# Auto run migrations
AUTO_MIGRATE="true"
